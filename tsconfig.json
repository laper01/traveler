{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",  // Add this line
    "paths": {
      "@/*": ["./*"]
    },
    "types": ["nativewind/types"]  // Move this out of "paths"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
